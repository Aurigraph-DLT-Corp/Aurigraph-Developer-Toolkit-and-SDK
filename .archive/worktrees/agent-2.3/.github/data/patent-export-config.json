  "folderStructures": {
    "byCategory": {
      "name": "Organization by Document Category",
      "folders": {
        "Technical Patents": {
          "description": "Core technical patent applications and granted patents",
          "categories": ["patent"],
          "priority": 1
        },
        "Business Method Patents": {
          "description": "Business method and process patents",
          "categories": ["patent"],
          "keywords": ["business method", "process", "workflow"],
          "priority": 2
        },
        "Invention Disclosures": {
          "description": "Internal invention disclosures and innovation records",
          "categories": ["invention"],
          "priority": 3
        },
        "Prior Art Research": {
          "description": "Prior art searches and competitive analysis",
          "categories": ["prior-art", "research"],
          "priority": 4
        },
        "Legal Documents": {
          "description": "Patent-related legal documents and agreements",
          "categories": ["legal"],
          "priority": 5
        },
        "Technical Specifications": {
          "description": "Detailed technical specifications and documentation",
          "categories": ["technical"],
          "priority": 6
        }
      }
    },
    "byTechnology": {
      "name": "Organization by Technology Area",
      "folders": {
        "Quantum Technology": {
          "description": "Quantum computing and cryptography patents",
          "technologies": ["quantum"],
          "priority": 1
        },
        "Consensus Algorithms": {
          "description": "Blockchain consensus and distributed systems",
          "technologies": ["consensus"],
          "priority": 2
        },
        "Cross-Chain Technology": {
          "description": "Interoperability and bridge technologies",
          "technologies": ["crossChain"],
          "priority": 3
        },
        "AI & Machine Learning": {
          "description": "AI-driven blockchain optimization",
          "technologies": ["ai"],
          "priority": 4
        },
        "Security & Cryptography": {
          "description": "Security protocols and cryptographic methods",
          "technologies": ["security"],
          "priority": 5
        },
        "Blockchain Core": {
          "description": "Core blockchain and DLT technologies",
          "technologies": ["blockchain"],
          "priority": 6
        }
      }
    },
    "byDate": {
      "name": "Organization by Creation Date",
      "folders": {
        "2024": {
          "description": "Documents created in 2024",
          "dateRange": "2024",
          "priority": 1
        },
        "2023": {
          "description": "Documents created in 2023",
          "dateRange": "2023",
          "priority": 2
        },
        "2022": {
          "description": "Documents created in 2022",
          "dateRange": "2022",
          "priority": 3
        },
        "Archive": {
          "description": "Older documents and historical records",
          "dateRange": "before-2022",
          "priority": 4
        }
      }
    },
    "flatStructure": {
      "name": "Flat Structure with Prefixes",
      "folders": {
        "All Documents": {
          "description": "All patent documents in a single folder with categorized naming",
          "namingConvention": "[CATEGORY]-[TECHNOLOGY]-[FILENAME]",
          "priority": 1
        }
      }
    }
  },
  "fileTypeHandling": {
    "supportedExtensions": [
      ".md", ".txt", ".pdf", ".doc", ".docx", ".rtf",
      ".tex", ".rst", ".adoc", ".html", ".xml",
      ".json", ".yaml", ".yml"
    ],
    "maxFileSize": "10MB",
    "excludePatterns": [
      "node_modules",
      ".git",
      "target",
      "build",
      "dist",
      ".class",
      ".jar",
      ".log",
      ".tmp"
    ],
    "specialHandling": {
      ".pdf": {
        "description": "PDF documents - uploaded as-is",
        "contentAnalysis": false
      },
      ".md": {
        "description": "Markdown documents - full content analysis",
        "contentAnalysis": true
      },
      ".doc": {
        "description": "Word documents - metadata extraction only",
        "contentAnalysis": false
      },
      ".json": {
        "description": "JSON configuration files - structure analysis",
        "contentAnalysis": true
      }
    }
  },
  "exportSettings": {
    "defaultMode": "incremental-update",
    "defaultTypes": "all",
    "defaultOrganization": "by-category",
    "includeMetadata": true,
    "generateArchive": true,
    "verifyUploads": true,
    "rateLimitDelay": 100,
    "maxRetries": 3,
    "chunkSize": 50
  },
  "metadataFields": {
    "document": [
      "fileName",
      "relativePath",
      "size",
      "lastModified",
      "category",
      "technology",
      "keywords",
      "contentScore"
    ],
    "export": [
      "exportId",
      "timestamp",
      "repository",
      "branch",
      "commit",
      "exportMode",
      "documentTypes",
      "folderOrganization"
    ],
    "googleDrive": [
      "driveId",
      "driveLink",
      "parentFolder",
      "uploadTimestamp",
      "fileSize"
    ]
  },
