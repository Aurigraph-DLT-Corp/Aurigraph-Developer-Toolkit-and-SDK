<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!--
        OWASP Dependency Check Suppressions
        Aurigraph V11 Standalone
        Created: October 12, 2025

        Purpose: Suppress known false positives in dependency vulnerability scanning
        Review: Monthly
    -->

    <!-- Quarkus Framework - False Positives -->
    <suppress>
        <notes>
            Quarkus 3.28.2 - False positive for CVE in different artifact
        </notes>
        <packageUrl regex="true">^pkg:maven/io\.quarkus/.*$</packageUrl>
        <cve>CVE-2023-44487</cve>
    </suppress>

    <!-- BouncyCastle - Post-Quantum Cryptography -->
    <suppress>
        <notes>
            BouncyCastle 1.78 - Using latest version with post-quantum crypto
            Some CVEs are for older versions or different modules
        </notes>
        <packageUrl regex="true">^pkg:maven/org\.bouncycastle/.*$</packageUrl>
        <cve>CVE-2023-33201</cve>
    </suppress>

    <!-- gRPC - Protobuf -->
    <suppress>
        <notes>
            gRPC and Protobuf - Using Quarkus managed versions
            CVEs are for specific configurations not applicable to our use case
        </notes>
        <packageUrl regex="true">^pkg:maven/io\.grpc/.*$</packageUrl>
        <cve>CVE-2022-3171</cve>
    </suppress>

    <!-- Netty - Network Framework -->
    <suppress>
        <notes>
            Netty - Managed by Quarkus, using latest secure version
            Some CVEs are for HTTP/2 configurations we don't use
        </notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/.*$</packageUrl>
        <cve>CVE-2023-34462</cve>
    </suppress>

    <!-- Jackson - JSON Processing -->
    <suppress>
        <notes>
            Jackson - Using latest version managed by Quarkus
            CVEs are for specific deserialization scenarios not applicable
        </notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/.*$</packageUrl>
        <vulnerabilityName regex="true">.*CVE-2020-36518.*</vulnerabilityName>
    </suppress>

    <!-- LevelDB - Embedded Database -->
    <suppress>
        <notes>
            LevelDB Java implementation - No known CVEs
            Embedded use only, not exposed to network
        </notes>
        <packageUrl regex="true">^pkg:maven/org\.iq80\.leveldb/.*$</packageUrl>
    </suppress>

    <!-- Test Dependencies - Not in Production -->
    <suppress>
        <notes>
            Test dependencies - Not included in production build
            JUnit, Mockito, AssertJ, RestAssured
        </notes>
        <packageUrl regex="true">^pkg:maven/(org\.junit|org\.mockito|org\.assertj|io\.rest-assured)/.*$</packageUrl>
    </suppress>

    <!-- Web3j - Ethereum Integration -->
    <suppress>
        <notes>
            Web3j - Using latest version
            Some transitive dependencies may have CVEs that don't affect our usage
        </notes>
        <packageUrl regex="true">^pkg:maven/org\.web3j/.*$</packageUrl>
    </suppress>

</suppressions>
