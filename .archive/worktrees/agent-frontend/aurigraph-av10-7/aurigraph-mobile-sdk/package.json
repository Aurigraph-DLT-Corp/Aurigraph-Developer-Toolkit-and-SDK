{
  "name": "@aurigraph/mobile-sdk",
  "version": "11.0.0",
  "description": "Aurigraph DLT Mobile SDK - Complete blockchain functionality for mobile developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "ios/**/*",
    "android/**/*",
    "react-native/**/*",
    "flutter/**/*",
    "shared/**/*",
    "docs/**/*",
    "examples/**/*",
    "!**/*.test.*",
    "!**/node_modules",
    "!**/coverage",
    "!**/.DS_Store"
  ],
  "scripts": {
    "build": "npm run build:shared && npm run build:platforms",
    "build:shared": "tsc -p shared/tsconfig.json",
    "build:platforms": "npm run build:ios && npm run build:android && npm run build:react-native && npm run build:flutter",
    "build:ios": "cd ios && xcodebuild -workspace AurigraphSDK.xcworkspace -scheme AurigraphSDK -configuration Release -destination generic/platform=iOS build",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:react-native": "cd react-native && npm run prepack",
    "build:flutter": "cd flutter && flutter packages get && flutter packages pub publish --dry-run",
    "test": "jest --config tests/jest.config.js",
    "test:unit": "jest --config tests/jest.config.js --selectProjects=\"Unit Tests\"",
    "test:integration": "jest --config tests/jest.config.js --selectProjects=\"Integration Tests\"",
    "test:e2e": "jest --config tests/jest.config.js --selectProjects=\"E2E Tests\"",
    "test:performance": "jest --config tests/jest.config.js --selectProjects=\"Performance Tests\"",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "docs:build": "typedoc --out docs/api shared/",
    "docs:serve": "serve docs/",
    "demo:ios": "cd demos/ios-demo && xcodebuild -workspace AurigraphDemo.xcworkspace -scheme AurigraphDemo -destination 'platform=iOS Simulator,name=iPhone 14' build",
    "demo:android": "cd demos/android-demo && ./gradlew assembleDebug",
    "demo:react-native": "cd demos/react-native-demo && npx react-native run-ios",
    "demo:flutter": "cd demos/flutter-demo && flutter run",
    "security:scan": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "release:prepare": "npm run build && npm run test && npm run security:scan",
    "release:ios": "cd ios && pod trunk push AurigraphSDK.podspec",
    "release:android": "cd android && ./gradlew publishReleasePublicationToSonatypeRepository",
    "release:react-native": "cd react-native && npm publish",
    "release:flutter": "cd flutter && flutter packages pub publish",
    "release:all": "npm run release:ios && npm run release:android && npm run release:react-native && npm run release:flutter",
    "clean": "rimraf dist coverage node_modules */node_modules */dist */coverage",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aurigraph-DLT-Corp/Aurigraph-DLT.git",
    "directory": "aurigraph-mobile-sdk"
  },
  "keywords": [
    "blockchain",
    "cryptocurrency",
    "aurigraph",
    "mobile",
    "sdk",
    "ios",
    "android",
    "react-native",
    "flutter",
    "quantum-resistant",
    "defi",
    "cross-chain",
    "bridge",
    "wallet",
    "biometric",
    "offline"
  ],
  "author": "Aurigraph DLT Corp <dev@aurigraph.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aurigraph-DLT-Corp/Aurigraph-DLT/issues"
  },
  "homepage": "https://github.com/Aurigraph-DLT-Corp/Aurigraph-DLT#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.68.0",
    "flutter": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "flutter": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "jest-junit": "^14.0.0",
    "jest-html-reporter": "^3.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "serve": "^14.0.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.24.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/aurigraph"
  },
  "contributors": [
    {
      "name": "Aurigraph DLT Team",
      "email": "dev@aurigraph.io",
      "url": "https://aurigraph.io"
    }
  ],
  "directories": {
    "doc": "docs",
    "test": "tests",
    "example": "demos"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}