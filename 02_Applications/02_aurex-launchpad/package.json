{
  "name": "aurex-launchpad",
  "version": "2.1.0",
  "description": "Aurex Launchpadâ„¢ - ESG Assessment & Sustainability Management",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main_enhanced:app --host 0.0.0.0 --port 8001 --reload",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && pip install -r requirements.txt",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm run preview -- --host 0.0.0.0 --port 3001",
    "start:backend": "cd backend && python -m uvicorn main_enhanced:app --host 0.0.0.0 --port 8001",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && python -m pytest",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && python -m flake8 .",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aurigraph-DLT-Corp/aurex-trace-platform.git"
  },
  "keywords": [
    "aurex",
    "launchpad",
    "esg",
    "assessment",
    "sustainability",
    "ghg",
    "carbon"
  ],
  "author": "Aurigraph",
  "license": "PROPRIETARY"
}
