{
  "name": "@aurigraph/sdk",
  "version": "1.0.0",
  "description": "Official Aurigraph DLT Platform SDK for third-party integrations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/AurigraphClient.js",
      "require": "./dist/client/AurigraphClient.js",
      "types": "./dist/client/AurigraphClient.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/AuthManager.js",
      "require": "./dist/auth/AuthManager.js",
      "types": "./dist/auth/AuthManager.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublish": "npm run build && npm run test:run"
  },
  "keywords": [
    "aurigraph",
    "blockchain",
    "dlt",
    "sdk",
    "api",
    "integration"
  ],
  "author": "Aurigraph DLT",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "jsonwebtoken": "^9.1.2",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aurigraph-DLT-Corp/Aurigraph-DLT.git",
    "directory": "aurigraph-v11-standalone/sdk"
  },
  "bugs": {
    "url": "https://github.com/Aurigraph-DLT-Corp/Aurigraph-DLT/issues"
  },
  "homepage": "https://aurigraph.io/sdk"
}
