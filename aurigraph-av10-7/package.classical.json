{
  "name": "aurigraph-av10-classical",
  "version": "10.35.0-classical",
  "description": "Aurigraph V10 Blockchain Platform - Classical CPU/GPU Version",
  "main": "dist/index-classical.js",
  "scripts": {
    "build": "npm run clean && npm run compile:classical",
    "clean": "rimraf dist",
    "compile:classical": "tsc -p tsconfig.classical.json",
    "start": "node dist/index-classical.js",
    "dev": "nodemon --exec ts-node src/index-classical.ts",
    "test": "jest --config jest.classical.config.js",
    "test:coverage": "jest --config jest.classical.config.js --coverage",
    "docker:build": "docker build -f Dockerfile.classical -t aurigraph-classical:latest .",
    "docker:run": "docker run -p 3100:3100 aurigraph-classical:latest",
    "gpu:check": "node scripts/check-gpu.js",
    "benchmark": "node scripts/benchmark-classical.js"
  },
  "keywords": [
    "blockchain",
    "gpu-computing",
    "high-performance",
    "distributed-ledger",
    "DLT",
    "cryptography",
    "tensorflow",
    "cuda"
  ],
  "dependencies": {
    "@tensorflow/tfjs-node-gpu": "^4.28.0",
    "axios": "^1.7.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.0",
    "winston": "^3.11.0",
    "cuda": "^0.2.0",
    "opencl": "^0.1.0",
    "gpu.js": "^2.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aurigraph-DLT-Corp/Aurigraph-Classical.git"
  },
  "hardware": {
    "minimum": {
      "cpu": "32 cores",
      "ram": "256GB",
      "gpu": "2x NVIDIA RTX 4090",
      "storage": "10TB SSD"
    },
    "recommended": {
      "cpu": "64+ cores AMD EPYC",
      "ram": "512GB+",
      "gpu": "4x NVIDIA A100",
      "storage": "50TB NVMe"
    },
    "production": {
      "cpu": "128+ cores",
      "ram": "1TB+",
      "gpu": "8x NVIDIA H100",
      "storage": "100TB NVMe",
      "network": "100Gbps"
    }
  }
}