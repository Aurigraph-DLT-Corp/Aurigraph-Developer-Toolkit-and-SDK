{
  "name": "aurigraph-av10-18",
  "version": "10.18.0",
  "description": "Next-generation quantum-resilient DLT platform with 5M+ TPS, autonomous compliance, and universal interoperability",
  "main": "dist/index-simple.js",
  "scripts": {
    "build": "tsc",
    "build:classical": "rimraf dist-classical && tsc -p tsconfig.classical.json",
    "start": "npx ts-node src/index-classical-simple.ts",
    "start:default": "npm start",
    "start:classical": "npx ts-node src/index-classical-simple.ts",
    "start:classical:full": "npm run build:classical && node dist-classical/index-classical.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit --coverage",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance --testTimeout=180000",
    "test:regression": "jest --testPathPattern=regression",
    "test:security": "npm audit && snyk test || true",
    "test:quick": "./scripts/testing/run-test-suite.sh quick",
    "test:all": "./scripts/testing/run-test-suite.sh all",
    "test:ci": "./scripts/testing/run-test-suite.sh ci",
    "test:report": "node scripts/testing/generate-comprehensive-report.js",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "docker:build": "docker-compose -f docker-compose.av10-7.yml build",
    "docker:up": "docker-compose -f docker-compose.av10-7.yml up -d",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network testnet",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network mainnet",
    "benchmark": "node scripts/benchmark.js",
    "migrate:v9-to-v10": "node scripts/migration/v9-to-v10.js",
    "ui:install": "cd ui && npm install",
    "ui:dev": "cd ui && npm run dev",
    "ui:build": "cd ui && npm run build",
    "start:full": "npm run build && npm start & cd ui && npm run dev",
    "deploy:dev4": "ts-node src/deploy-dev4.ts",
    "validate:dev4": "ts-node src/validate-dev4.ts",
    "dev4": "npm run validate:dev4"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.1.3",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@octokit/rest": "^22.0.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "@wormhole-foundation/sdk": "^3.4.5",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-prometheus-middleware": "^1.2.0",
    "http-proxy-middleware": "^3.0.5",
    "inversify": "^6.0.2",
    "lodash": "^4.17.21",
    "mqtt": "^5.14.0",
    "prom-client": "^13.2.0",
    "reflect-metadata": "^0.2.0",
    "typescript": "^5.3.0",
    "uuid": "^9.0.1",
    "websocket-as-promised": "^3.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@playwright/test": "^1.57.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.13.0",
    "hardhat": "^2.19.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "snyk": "^1.1266.0",
    "solidity-coverage": "^0.8.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "blockchain",
    "quantum-safe",
    "zero-knowledge",
    "cross-chain",
    "defi",
    "web3",
    "cryptography",
    "consensus",
    "distributed-ledger",
    "5m-tps",
    "autonomous-compliance",
    "quantum-native",
    "enterprise-dlt"
  ],
  "author": "Aurigraph Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurigraph/av10-18"
  }
}
